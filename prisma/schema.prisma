datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  username  String @unique
  name  String?
  phone_number  String?
  skill_set  String?
  hobby String?
  createdAt DateTime @default(now())
  updatedAt DateTime? @updatedAt
}